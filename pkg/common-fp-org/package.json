{
  "name": "@common-fp/common-fp-org",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "author": "Phil Olson",
  "license": "SEE LICENSE IN license.txt",
  "scripts": {
    "dev": "./scripts/dev-watch.sh && IS_DEVELOPING=true next dev --experimental-https",
    "build": "next build && node scripts/create-sitemap.mjs",
    "build-resources": "node --no-warnings=ExperimentalWarning scripts/build-resources/index.mjs",
    "start": "next start",
    "lint": "eslint 'misc/**/*.mjs' 'scripts/**/*.mjs' code-examples app next.config.mjs",
    "preview": "IS_PREVIEW=true serve --ssl-cert ./certificates/localhost.pem --ssl-key ./certificates/localhost-key.pem out",
    "test-cli": "mocha --recursive test/cases",
    "test": "pnpm test-cli > ../../test-results/common-fp-org.txt",
    "test-one": "mocha",
    "call-prettier": "prettier --ignore-path ../../.gitignore --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown .",
    "prettier-check": "pnpm call-prettier --check",
    "prettier-write": "pnpm call-prettier --write --list-different"
  },
  "imports": {
    "#code-example-transforms": "./misc/code-example-transforms.mjs",
    "#code-examples/*": "./app/built/code-examples/*",
    "#test/*": "./test/*.mjs"
  },
