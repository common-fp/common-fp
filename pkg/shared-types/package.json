{
  "name": "@common-fp/shared-types",
  "description": "Shared types for Common FP libraries",
  "private": false,
  "version": "0.1.1",
  "sideEffects": false,
  "type": "module",
  "types": "dist/index.d.cts",
  "main": "dist/index.d.cts",
  "scripts": {
    "build": "node scripts/build.mjs",
    "lint": "pnpm eslint 'src/**/*' 'scripts/**/*' 'test/**/*'",
    "prepack": "sh '../../scripts/prepack.sh' shared-types",
    "postpack": "sh '../../scripts/postpack.sh' shared-types",
    "test-full": "tstyche",
    "test-lite": "tstyche --target 5.9"
  },
  "exports": {
    ".": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.cts"
    },
    "./*": {
      "import": "./dist/*.d.ts",
      "require": "./dist/*.d.cts"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#src/*": "./src/*.d.ts"
  },
  "keywords": [
    "common",
    "fp",
    "shared",
    "types"
  ],
  "author": "Phil Olson",
  "license": "SEE LICENSE IN license.txt",
  "devDependencies": {
    "@common-fp/eslint-config": "workspace:*",
    "@common-fp/prettier-config": "workspace:*",
    "@common-fp/tsconfig": "workspace:*",
    "cpy": "^11.1.0",
    "del": "^7.1.0",
    "eslint": "^9.32.0",
    "globby": "^14.0.2",
    "make-dir": "^5.0.0",
    "prettier": "^3.3.3",
    "tstyche": "^4.3.0",
    "typescript": "^5.6.3"
  },
  "bugs": {
    "url": "https://github.com/common-fp/common-fp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/common-fp/common-fp.git",
    "directory": "pkg/shared-types"
  },
  "homepage": "https://github.com/common-fp/common-fp/tree/dev/pkg/shared-types",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "typescript": "5.x"
  }
}
