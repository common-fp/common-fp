{
  "name": "@common-fp/eslint-config",
  "private": false,
  "version": "0.1.1",
  "description": "The eslint config used in common-fp libraries",
  "main": "base.mjs",
  "type": "module",
  "scripts": {
    "call-prettier": "prettier --ignore-unknown .",
    "prettier-check": "pnpm call-prettier --check",
    "prettier-write": "pnpm call-prettier --write --list-different"
  },
  "exports": {
    ".": "./base.mjs",
    "./base": "./base.mjs",
    "./base-with-ts": "./base-with-ts.mjs"
  },
  "keywords": [
    "common-fp"
  ],
  "author": "Phil Olson",
  "license": "SEE LICENSE IN license.txt",
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import-x": "^4.16.1",
    "globals": "^15.11.0"
  },
  "devDependencies": {
    "@common-fp/prettier-config": "workspace:*",
    "eslint": "^9.32.0",
    "prettier": "catalog:"
  },
  "optionalDependencies": {
    "@typescript-eslint/parser": "^8.40.0",
    "typescript-eslint": "^8.40.0"
  },
  "peerDependencies": {
    "eslint": "^9.32.0"
  },
  "prettier": "@common-fp/prettier-config"
}
