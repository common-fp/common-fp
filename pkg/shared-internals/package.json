{
  "name": "@common-fp/shared-internals",
  "description": "Code used to build common-fp utilities",
  "private": false,
  "version": "0.1.2",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "type": "module",
  "types": "./dist/index.d.cts",
  "scripts": {
    "build": "node scripts/build/index.mjs",
    "coverage": "c8 pnpm test-lite",
    "coverage-check": "c8 --check-coverage --100 pnpm test-lite",
    "coverage-one": "sh scripts/coverage-one.sh",
    "lint": "eslint 'scripts/**/*.mjs' src test",
    "lint-built": "eslint 'dist/**/*.cjs'",
    "prepack": "sh '../../scripts/prepack.sh' shared-internals",
    "postpack": "sh '../../scripts/postpack.sh'",
    "test-full-cli": "testSupportedNodeVersions mocha --ui tdd --require ./test/init.mjs --recursive test/cases",
    "test-full": "pnpm test-full-cli > ../../test-results/shared-internals.txt 2>&1",
    "test-lite": "mocha --recursive test/cases",
    "test-one": "mocha",
    "call-prettier": "prettier --ignore-path ../../.gitignore --ignore-unknown .",
    "prettier-check": "pnpm call-prettier --check",
    "prettier-write": "pnpm call-prettier --write"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    },
    "./deps/*": null,
    "./package.json": "./package.json"
  },
  "files": [
    "license.txt",
    "readme.md",
    "dist"
  ],
  "imports": {
    "#src/*": "./src/*.mjs",
    "#test/*": "./test/*.mjs"
  },
  "keywords": [
    "common-fp"
  ],
  "author": "Phil Olson",
  "license": "SEE LICENSE IN license.txt",
  "devDependencies": {
    "@common-fp/eslint-config": "workspace:*",
    "@common-fp/prettier-config": "workspace:*",
    "@common-fp/shared-internals-types": "workspace:*",
    "@common-fp/test-utils": "workspace:*",
    "@common-fp/tsconfig": "workspace:*",
    "async": "^3.2.6",
    "c8": "^10.1.3",
    "chai": "^5.1.2",
    "change-case": "^5.4.4",
    "cpy": "^11.1.0",
    "dedent": "^1.5.3",
    "del": "^7.1.0",
    "eslint": "^9.32.0",
    "fibble": "^1.2.3",
    "globby": "^14.0.2",
    "make-dir": "^5.0.0",
    "mocha": "^10.6.0",
    "monocart-coverage-reports": "^2.12.6",
    "nvexeca": "^11.0.4",
    "prettier": "catalog:"
  },
  "bugs": {
    "url": "https://github.com/common-fp/common-fp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/common-fp/common-fp.git",
    "directory": "pkg/shared-internals"
  },
  "homepage": "https://github.com/common-fp/common-fp/tree/dev/pkg/shared-internals",
  "peerDependenciesMeta": {
    "@common-fp/shared-internals-types": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@common-fp/shared-internals-types": "workspace:^"
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": "@common-fp/prettier-config",
  "mocha": {
    "node-option": [
      "experimental-import-meta-resolve",
      "import=fibble/register"
    ],
    "ui": "tdd",
    "require": "test/init.mjs"
  },
  "c8": {
    "include": [
      "src/*.mjs"
    ],
    "exclude": [
      "src/common-types.mjs"
    ],
    "experimental-monocart": true
  }
}
