{
  "name": "common-fp-types",
  "sideEffects": false,
  "description": "Types for common-fp",
  "version": "0.1.3",
  "type": "module",
  "author": "Phil Olson",
  "license": "SEE LICENSE IN license.txt",
  "main": "",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "lint": "eslint test-types test scripts src",
    "postpack": "sh '../../scripts/postpack.sh'",
    "prepack": "sh '../../scripts/prepack.sh' common-fp-types",
    "test-full-cli": "pnpm validate-tests && testSupportedTSVersions",
    "test-full": "pnpm test-full-cli > ../../test-results/common-fp-types.txt 2>&1",
    "test-lite": "pnpm validate-tests && tstyche --target 5.9",
    "validate-tests": "mocha --recursive test",
    "call-prettier": "prettier --ignore-path ../../.gitignore --ignore-unknown .",
    "prettier-check": "pnpm call-prettier --check",
    "prettier-write": "pnpm call-prettier --write --list-different"
  },
  "exports": {
    ".": "./dist/index.d.ts",
    "./*": "./dist/*.d.ts"
  },
  "imports": {
    "#src/*": "./src/*.d.ts",
    "#dist/*": "./dist/*.d.ts"
  },
  "devDependencies": {
    "@common-fp/eslint-config": "workspace:*",
    "@common-fp/prettier-config": "workspace:*",
    "@common-fp/test-utils": "workspace:*",
    "@common-fp/tsconfig": "workspace:*",
    "chai": "^5.1.2",
    "change-case": "^5.4.4",
    "cpy": "^11.1.0",
    "del": "^7.1.0",
    "eslint": "^9.32.0",
    "globby": "^14.0.2",
    "make-dir": "^5.0.0",
    "mocha": "^11.7.1",
    "prettier": "catalog:",
    "tstyche": "catalog:",
    "typescript-eslint": "^8.39.0"
  },
  "bugs": {
    "url": "https://github.com/common-fp/common-fp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/common-fp/common-fp.git",
    "directory": "pkg/common-fp-types"
  },
  "homepage": "https://github.com/common-fp/common-fp/tree/dev/pkg/common-fp-types",
  "keywords": [
    "common",
    "fp",
    "types"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@common-fp/shared-types": "workspace:^"
  },
  "peerDependencies": {
    "typescript": "5.x"
  },
  "prettier": "@common-fp/prettier-config",
  "mocha": {
    "ui": "tdd"
  }
}
